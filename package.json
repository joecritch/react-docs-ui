{
  "name": "react-docs-ui",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "sass": "node-sass src/scss/ -o dist/css/ --recursive --source-map true",
    "watch:sass": "node-sass src/scss/ -o dist/css/ --watch --recursive --source-map true",
    "server": "http-server -p 8081",
    "start": "npm-run-all server watch:sass"
  },
  "devDependencies": {
    "http-server": "^0.10.0",
    "node-sass": "^4.5.3"
  }
}
